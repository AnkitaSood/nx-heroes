{
  "version": 1,
  "projects": {
    "data-access-heroes": {
      "root": "libs/data-access-heroes",
      "sourceRoot": "libs/data-access-heroes/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/data-access-heroes/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/data-access-heroes"],
          "options": {
            "jestConfig": "libs/data-access-heroes/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "feature-dashboard": {
      "projectType": "library",
      "root": "libs/feature-dashboard",
      "sourceRoot": "libs/feature-dashboard/src",
      "prefix": "tour-of-heroes",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/feature-dashboard"],
          "options": {
            "jestConfig": "libs/feature-dashboard/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/feature-dashboard/src/**/*.ts",
              "libs/feature-dashboard/src/**/*.html"
            ]
          }
        }
      }
    },
    "feature-hero-details": {
      "projectType": "library",
      "root": "libs/feature-hero-details",
      "sourceRoot": "libs/feature-hero-details/src",
      "prefix": "tour-of-heroes",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/feature-hero-details"],
          "options": {
            "jestConfig": "libs/feature-hero-details/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/feature-hero-details/src/**/*.ts",
              "libs/feature-hero-details/src/**/*.html"
            ]
          }
        }
      }
    },
    "feature-hero-search": {
      "projectType": "library",
      "root": "libs/tour-of-heroes/feature-hero-search",
      "sourceRoot": "libs/tour-of-heroes/feature-hero-search/src",
      "prefix": "tour-of-heroes",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/tour-of-heroes/feature-hero-search"],
          "options": {
            "jestConfig": "libs/tour-of-heroes/feature-hero-search/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/tour-of-heroes/feature-hero-search/src/**/*.ts",
              "libs/tour-of-heroes/feature-hero-search/src/**/*.html"
            ]
          }
        }
      }
    },
    "feature-heroes": {
      "projectType": "library",
      "root": "libs/feature-heroes",
      "sourceRoot": "libs/feature-heroes/src",
      "prefix": "tour-of-heroes",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/feature-heroes"],
          "options": {
            "jestConfig": "libs/feature-heroes/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/feature-heroes/src/**/*.ts",
              "libs/feature-heroes/src/**/*.html"
            ]
          }
        }
      }
    },
    "shared-ui-kit": {
      "projectType": "library",
      "root": "libs/shared/ui-kit",
      "sourceRoot": "libs/shared/ui-kit/src",
      "prefix": "tour-of-heroes",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/ui-kit"],
          "options": {
            "jestConfig": "libs/shared/ui-kit/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/ui-kit/src/**/*.ts",
              "libs/shared/ui-kit/src/**/*.html"
            ]
          }
        }
      }
    },
    "tour-of-heroes": {
      "projectType": "application",
      "root": "apps/tour-of-heroes",
      "sourceRoot": "apps/tour-of-heroes/src",
      "prefix": "demo",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/tour-of-heroes",
            "index": "apps/tour-of-heroes/src/index.html",
            "main": "apps/tour-of-heroes/src/main.ts",
            "polyfills": "apps/tour-of-heroes/src/polyfills.ts",
            "tsConfig": "apps/tour-of-heroes/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/tour-of-heroes/src/favicon.ico",
              "apps/tour-of-heroes/src/assets"
            ],
            "styles": ["apps/tour-of-heroes/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/tour-of-heroes/src/environments/environment.ts",
                  "with": "apps/tour-of-heroes/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "tour-of-heroes:build:production"
            },
            "development": {
              "browserTarget": "tour-of-heroes:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "tour-of-heroes:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/tour-of-heroes/src/**/*.ts",
              "apps/tour-of-heroes/src/**/*.html"
            ],
            "tsConfig": [
              "apps/tour-of-heroes/tsconfig.app.json",
              "apps/tour-of-heroes/tsconfig.spec.json"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/tour-of-heroes"],
          "options": {
            "jestConfig": "apps/tour-of-heroes/jest.config.js",
            "passWithNoTests": true,
            "tsConfig": "apps/tour-of-heroes/tsconfig.spec.json",
            "karmaConfig": "apps/tour-of-heroes/karma.conf.js"
          }
        }
      }
    },
    "tour-of-heroes-e2e": {
      "root": "apps/tour-of-heroes-e2e",
      "sourceRoot": "apps/tour-of-heroes-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/tour-of-heroes-e2e/cypress.json",
            "tsConfig": "apps/tour-of-heroes-e2e/tsconfig.e2e.json",
            "devServerTarget": "tour-of-heroes:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "tour-of-heroes:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/tour-of-heroes-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "tour-of-heroes-feature-header": {
      "projectType": "library",
      "root": "libs/tour-of-heroes/feature-header",
      "sourceRoot": "libs/tour-of-heroes/feature-header/src",
      "prefix": "tour-of-heroes",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/tour-of-heroes/feature-header"],
          "options": {
            "jestConfig": "libs/tour-of-heroes/feature-header/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/tour-of-heroes/feature-header/src/**/*.ts",
              "libs/tour-of-heroes/feature-header/src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "tour-of-heroes"
}
